{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "boomerang/bonus-form",
  "version": "0.1.0",
  "title": "Bonus Form",
  "category": "widgets",
  "icon": "forms",
  "description": "A block to display a pseudo-form for bonuses",
  "example": {},
  "supports": {
    "html": false
  },
  "attributes": {
    "title": {
      "type": "string",
      "default": "Введите заголовок"
    },
    "buttonText": {
      "type": "string",
      "default": "Tекст кнопки"
    },
    "bonuses": {
      "type": "array",
      "default": []
    }
  },
  "textdomain": "bonus-form",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js"
}