{
    "name": "kromvel/boomerang-wp",
    "description": "Composer package for managing dependencies and autoloading in a WordPress project.",
    "type": "project",
    "autoload": {
        "psr-4": {
            "Boomerang\\": "wp-content/"
        }
    },
    "authors": [
        {
            "name": "Nikita",
            "email": "kromveln@gmail.com"
        }
    ],
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "szepeviktor/phpstan-wordpress": "^2.0",
        "phpstan/extension-installer": "^1.4"
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
